      },
      {
        "name": "price",
        "widgetClass": "omna.form.field.NumberField",
        "showInGrid": true,
        "includeInSearch": true,
        "accessInAddForm": "W",
        "accessInEditForm": "W",
        "required": true,
        "minimum": 0,
        "maximumFractionDigits": 2,
        "minimumFractionDigits": 2,
        "prefix": "$",
        "gridColumnWidth": 100
      },
      {
        "name": "original_price",
        "widgetClass": "omna.form.field.NumberField",
        "showInGrid": true,
        "includeInSearch": true,
        "accessInAddForm": "W",
        "accessInEditForm": "W",
        "required": true,
        "minimum": 0,
        "maximumFractionDigits": 2,
        "minimumFractionDigits": 2,
        "prefix": "$",
        "gridColumnWidth": 100
      },
      {
        "name": "quantity",
        "widgetClass": "omna.form.field.NumberField",
        "showInGrid": true,
        "includeInSearch": true,
        "accessInAddForm": "W",
        "accessInEditForm": "W",
        "required": true,
        "minimum": 0,
        "maximumFractionDigits": 0,
        "minimumFractionDigits": 0,
        "gridColumnWidth": 100
      },
      {
        "name": "integrations",
        "showInGrid": true,
        "sortable": false,
        "cellRendererClass": "omna.table.cellrenderer.String",
        "cellClass": "integrations",
        "isTemplate": true,
        "isRichText": true,
        "i18n": {
          "subCatalog": "Titles",
          "name": "INTEGRATIONS"
        }
      },
      {
        "name": "integration",
        "includeInSearch": true,
        "customModelName": "integration_id",
        "widgetClass": "omna.form.field.remote.integrations.SelectBox",
        "disableUnauthorized": false
      },
      {
        "name": "package.weight",
        "widgetClass": "omna.form.field.NumberField",
        "accessInAddForm": "W",
        "accessInEditForm": "W",
        "required": false,
        "minimum": 0,
        "maximumFractionDigits": 2,
        "minimumFractionDigits": 2,
        "postfix": " kg"
