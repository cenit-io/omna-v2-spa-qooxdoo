        "minimum": 0,
        "maximumFractionDigits": 0,
        "minimumFractionDigits": 0,
        "postfix": " cm"
      },
      {
        "name": "package.length",
        "widgetClass": "omna.form.field.NumberField",
        "accessInAddForm": "W",
        "accessInEditForm": "W",
        "required": true,
        "minimum": 0,
        "maximumFractionDigits": 0,
        "minimumFractionDigits": 0,
        "postfix": " cm"
      },
      {
        "name": "package.width",
        "widgetClass": "omna.form.field.NumberField",
        "accessInAddForm": "W",
