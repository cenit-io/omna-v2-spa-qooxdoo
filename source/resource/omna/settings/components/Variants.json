[
  {
    "id": "Variants",
    "i18n": "Variants",
    "widgetClass": "omna.management.DataGridRestService",
    "requestManagementClass": "omna.request.Variants",
    "rowHeight": 60,
    "formWidth": 800,
    "searchFormWidth": 600,
    "actions": [
      "omna.action.Reload",
      "omna.action.Separator",
      "omna.action.Add",
      "omna.action.Edit",
      "omna.action.Details",
      "omna.action.variant.integrations.Link",
