    "requestManagementClass": "omna.request.Tenants",
    "baseParams": {
      "with_details": true
    },
    "actions": [
      "omna.action.Reload",
      "omna.action.Separator",
      "omna.action.Add",
      "omna.action.tenant.Switch",
      "omna.action.tenant.Startup",
      "omna.action.Remove"
    ],
    "fields": [
      {
        "name": "name",
        "widgetClass": "omna.form.field.TextField",
        "showInGrid": true,
        "accessInAddForm": "W",
        "accessInEditForm": "R",
        "required": true,
        "minLength": 2,
        "maxLength": 50,
        "pattern": "\\w+[\\w\\-@&#]"
      },
      {
        "name": "is_ready_to_omna",
        "cellRendererClass": "omna.table.cellrenderer.Boolean",
        "showInGrid": true,
        "gridColumnWidth": 100
      },
      {
        "name": "created_at",
        "showInGrid": true,
        "cellRendererClass": "omna.table.cellrenderer.Date",
        "gridColumnWidth": 150
      },
      {
        "name": "updated_at",
        "showInGrid": true,
        "cellRendererClass": "omna.table.cellrenderer.Date",
        "gridColumnWidth": 150
      }
    ]
  }
]